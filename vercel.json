{
    "version": 2,
    "builds": [
      { "src": "backend/app.py", "use": "@vercel/python" },
      { "src": "src/index.js", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "backend/app.py" },
      { "src": "/(.*)", "dest": "dist/$1" }
    ]
  }