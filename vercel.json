{
    "version": 2,
    "builds": [
      {
        "src": "backend/api/app.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9"
        }
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "dist"
        }
      }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/api/app.py"
        },
        {
            "src": "/(.*)",
            "dest": "dist/$1"
        }
    ],
    "env": {
      "WEBSOCKET_URL": "wss://fauna-lmsh-timer.vercel.app",
      "BACKEND": "https://fauna-lmsh-timer.vercel.app/api"
    }
  }